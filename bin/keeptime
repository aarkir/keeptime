#!/usr/bin/env node

(function() {
  "use strict";

  var keeptime = require("../lib/keeptime");
  var argv = require('minimist' (process.argv.slice(2)));

  // _ is the list of unassociated args under minimist
  if (argv._.length) {
    switch (argv._[0]) {
      case "help":
        break;
      case "starttimer":
        break;
      case "endtimer":
        break;
      case "addback":
        break;
      case "today":
        break;
      case "total":
        break;
      default:
        break;
    }
  } else {
    console.error("No file passed in!");
  }

})();
